{
    "name": "OSMFoundation",
    "author": "nebulon42",
    "url": "https://github.com/osmfoundation/osmf-mediawiki-skin",
    "descriptionmsg": "osmf-desc",
    "namemsg": "skinname-osmf",
    "license-name": "GPL-2.0+",
    "type": "skin",
    "manifest_version": 2,
    "requires": {
        "MediaWiki": ">= 1.29.0"
    },
    "ValidSkinNames": {
        "osmfoundation": "OSMFoundation"
    },
    "AutoloadClasses": {
        "SkinOSMFoundation": "OSMFoundationSkin.php",
        "OSMFoundationTemplate": "OSMFoundationTemplate.php"
    },
    "MessagesDirs": {
        "OSMF": "i18n"
    },
    "ResourceModules": {
        "skins.osmfoundation": {
            "class": "ResourceLoaderSkinModule",
            "features": {
                "interface": true,
                "logo": true
            },
            "targets": [ "desktop", "mobile" ],
            "styles": {
                "osmfoundation.less": {}
            }
        },
        "skins.osmfoundation.js": {
            "scripts": [
                "osmfoundation.js"
            ],
            "position": "bottom"
        }
    },
    "ResourceFileModulePaths": {
        "localBasePath": "",
        "remoteSkinPath": "OSMFoundation"
    }
}
